{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@atoms/*": [
        "src/components/atoms/*"
      ],
      "@screens/*": [
        "src/components/screens/*"
      ],
      "@molecules/*": [
        "./src/components/molecules/*"
      ],
      "@templates/*": [
        "src/components/templates/*"
      ],
      "@storage/*": [
        "./src/storage/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  }
}
