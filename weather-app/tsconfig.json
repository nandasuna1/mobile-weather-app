{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@atoms/*": [
        "./src/atoms/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@molecules/*": [
        "./src/molecules/*"
      ],
      "@templates/*": [
        "src/template/*"
      ],
      "@storage/*": [
        "./src/storage/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  }
}
